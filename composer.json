{
    "name": "mrsatik/cs",
    "minimum-stability": "stable",
    "keywords": [
        "verifier",
        "style",
        "sniffer",
        "code"
    ],
    "type": "library",
    "license": "MIT",
    "description": "Code style verifier",
    "require": {
        "php": "^7.2",
        "squizlabs/php_codesniffer": " 3.5.3"
    },
    "authors": [
        {
            "name": "Perchenok Alexey",
            "email": "silenthill.sat@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "mrsatik\\Sniff\\WhiteSpace\\": "src/standards/mrsatik/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "mrsatik\\CodestyleTest\\": "tests/"
        }
     },
    "bin": [
        "bin/mrsatik-phpcs",
        "bin/mrsatik-phpcbf"
    ],
    "require-dev": {
        "phpunit/phpunit": "^8.4"
    },
    "scripts": {
        "test": [
            "phpunit"
        ]
    },
    "config": {
        "secure-http":false
    }
}
